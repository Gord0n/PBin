@model PBin.Models.ViewModels.UserListViewModel
@{
    ViewBag.Title = "User List";
}
<div class="row">
    <div class="col-xs-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-9">
                        <h1>User List</h1>
                    </div>
                    <div class="col-xs-3 text-right">
                      
                    </div>
                </div>

            </div>
            <div class="panel-body">
                @for (int i = 0; i < Model.Users.Count; i++)
                {
                    <div class="row">
                        <div class="col-xs-12">
                            <p>@Model.Users[i].FirstName @Model.Users[i].LastName - <a href="@Url.Action("AdminDeleteUser", "Home", new { UserId = Model.Users[i].Id })">Remove</a></p>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>
<div id="feedback" class='col-xs-12' style='display:none'>@Model.sts</div>